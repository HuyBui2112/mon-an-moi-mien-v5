{
  "expo": {
    "name": "Mon-an-moi-mien-v2",
    "slug": "Mon-an-moi-mien-v2",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/viet-icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": null,
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "your.app.bundle",
      "splash": {
        "resizeMode": "native"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/viet-icon.png",
        "backgroundColor": "#ffffff"
      },
      "config": {
        "googleMaps": {
          "apiKey": "YOUR_GOOGLE_MAPS_API_KEY"
        }
      },
      "package": "com.monanmoimien.v2",
      "scheme": "monanmoimien",
      "splash": {
        "resizeMode": "native"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "813e424c-768f-4912-8a6c-a2d599d22d93"
      }
    },
    "fonts": [
      {
        "asset": "assets/fonts/DancingScript-Bold.ttf",
        "name": "DancingScript_700Bold"
      },
      {
        "asset": "assets/fonts/Pacifico-Regular.ttf",
        "name": "Pacifico_400Regular"
      }
    ],
    "plugins": ["expo-font"]
  }
}
